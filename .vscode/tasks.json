{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build",
            "type": "shell",
            "command": "msbuild",
            "args": [
                "${workspaceFolder}/DropTableEditor.sln",
                "/p:Configuration=Debug",
                "/t:Build",
                "/consoleloggerparameters:NoSummary"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "presentation": {
                "reveal": "silent",
                "panel": "shared"
            },
            "problemMatcher": "$msCompile"
        },
        {
            "label": "build-release",
            "type": "shell",
            "command": "msbuild",
            "args": [
                "${workspaceFolder}/DropTableEditor.sln",
                "/p:Configuration=Release",
                "/t:Build",
                "/consoleloggerparameters:NoSummary"
            ],
            "group": "build",
            "presentation": {
                "reveal": "silent",
                "panel": "shared"
            },
            "problemMatcher": "$msCompile"
        },
        {
            "label": "clean",
            "type": "shell",
            "command": "msbuild",
            "args": [
                "${workspaceFolder}/DropTableEditor.sln",
                "/t:Clean",
                "/consoleloggerparameters:NoSummary"
            ],
            "presentation": {
                "reveal": "silent",
                "panel": "shared"
            },
            "problemMatcher": "$msCompile"
        },
        {
            "label": "rebuild",
            "type": "shell",
            "command": "msbuild",
            "args": [
                "${workspaceFolder}/DropTableEditor.sln",
                "/p:Configuration=Debug",
                "/t:Rebuild",
                "/consoleloggerparameters:NoSummary"
            ],
            "group": "build",
            "presentation": {
                "reveal": "silent",
                "panel": "shared"
            },
            "problemMatcher": "$msCompile"
        }
    ]
}
